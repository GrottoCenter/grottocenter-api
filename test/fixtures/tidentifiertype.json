[
  {
    "id": "doi",
    "text": "digital object identifier",
    "regexp": "^.+$"
  },
  {
    "id": "isbn",
    "text": "international standard book number",
    "regexp": "^(97[89]-)?[0-9]{1,5}-[0-9]+-[0-9]+-[0-9]$"
  },
  {
    "id": "issn",
    "text": "international standard serial number",
    "regexp": "^[0-9]{4}-[0-9]{3}[0-9x]$"
  },
  {
    "id": "url",
    "text": "uniform resource locator",
    "regexp": "^[a-z]{2,5}:\/\/.+\\..+$"
  }
]